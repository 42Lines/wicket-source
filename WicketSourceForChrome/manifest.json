{
  "manifest_version": 1,
  "name": "WicketSource",
  "version": "1.1.2",
  "description": "Adds a Developer Tools sidebar with Wicket source code location for the selected DOM element.",
  "devtools_page": "devtools.html",
  "options_page": "options.html",
   "icons": { 
    "16": "go_icon.png",
   	"48": "icon48.png", 
   	"64": "icon64.png", 
   	"128": "icon128.png"  
  },
  "permissions":  [
    "http://*/*", "https://*/*", "http://localhost/*"
  ]
}
